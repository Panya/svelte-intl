{
  "name": "svelte-intl",
  "version": "0.1.0",
  "description": "Internationalize your Svelte apps",
  "main": "dist/svelte-intl.js",
  "module": "module.js",
  "scripts": {
    "clean": "rimraf dist/",
    "build": "npm run clean && rollup -c",
    "prepublish": "npm run lint && npm run size && npm run build",
    "test": "npm run build && jest",
    "lint": "eslint module.js",
    "size": "size-limit"
  },
  "keywords": [
    "svelte",
    "format-message",
    "intl",
    "i18n"
  ],
  "files": [
    "module.js",
    "dist"
  ],
  "author": "Mikhail Korepanov <panyakor@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/Panya/svelte-intl"
  },
  "bugs": {
    "url": "https://github.com/Panya/svelte-intl/issues"
  },
  "homepage": "https://github.com/Panya/svelte-intl",
  "dependencies": {
    "format-message": "6.2.1"
  },
  "devDependencies": {
    "eslint": "5.16.0",
    "jest": "24.8.0",
    "rimraf": "2.6.3",
    "rollup": "1.12.4",
    "rollup-plugin-buble": "0.19.6",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-node-resolve": "5.0.0",
    "size-limit": "1.3.5",
    "svelte": "2.16.1"
  },
  "size-limit": [
    {
      "path": "module.js",
      "limit": "6.5 KB"
    }
  ]
}
